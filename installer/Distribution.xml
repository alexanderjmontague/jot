<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="1">
    <title>Jot Helper</title>
    <welcome file="welcome.html"/>
    <conclusion file="conclusion.html"/>
    <pkg-ref id="com.jot.host"/>
    <options customize="never" require-scripts="false" hostArchitectures="arm64,x86_64"/>
    <choices-outline>
        <line choice="default">
            <line choice="com.jot.host"/>
        </line>
    </choices-outline>
    <choice id="default"/>
    <choice id="com.jot.host" visible="false">
        <pkg-ref id="com.jot.host"/>
    </choice>
    <pkg-ref id="com.jot.host" version="1.0.0" onConclusion="none">JotHelper-component.pkg</pkg-ref>
</installer-gui-script>
